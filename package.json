{
  "name": "leafeon",
  "version": "2.0.8",
  "description": "Client-sided and dependency-free Javascript routing library",
  "author": "Sundowndev",
  "main": "dist/leafeon",
  "types": "dist/leafeon.d.ts",
  "directories": {
    "doc": "docs",
    "src": "src"
  },
  "scripts": {
    "build": "npm run mjs && npm run cjs && npm run umd",
    "mjs": "tsc -d && mv dist/leafeon.js dist/leafeon.mjs",
    "cjs": "tsc -m commonjs",
    "umd": "rollup -i dist/leafeon.mjs -o dist/leafeon.umd.js -f umd -n leafeon",
    "test": "npm run build && webpack-cli test/routes.js -o test/bundle.js --mode=production",
    "release": "git push --follow-tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SundownDEV/leafeon.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "router",
    "javascript",
    "library",
    "front-end",
    "routing",
    "routerjs",
    "client-side-routing"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SundownDEV/leafeon/issues"
  },
  "homepage": "https://github.com/SundownDEV/leafeon#readme",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "rollup": "^0.66.6",
    "typescript": "^3.1.4",
    "typescript-eslint-parser": "^20.1.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.2"
  }
}
